text = "Ahem, an increased need in personel, so we've been conducting *quite* a lot of interviews."

[sub_resource type="Resource" id="Resource_ieyja"]
script = ExtResource("1_p6dvn")
speaker = "Interviewer"
text = "Anyways, welcome to RTN Management. You are..."

[sub_resource type="Resource" id="Resource_0udhu"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "I'm Doug, applying for the Associate Portfolio Manager position."

[sub_resource type="Resource" id="Resource_c40c5"]
script = ExtResource("1_p6dvn")
speaker = "Interviewer"
text = "Nice to meet you, Duck."

[sub_resource type="Resource" id="Resource_tryih"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "..."

[sub_resource type="Resource" id="Resource_3tju6"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "(...Duck?) (I said Doug, right? Is my enunciation that bad? Or maybe she misspoke.)"

[sub_resource type="Resource" id="Resource_u12gh"]
script = ExtResource("1_p6dvn")
speaker = "Interviewer"
text = "Duck's a fun name."

[sub_resource type="Resource" id="Resource_ikdcy"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "Okay, she definitely said Duck."

[sub_resource type="Resource" id="Resource_fw2oi"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "Should I correct her? But what if she takes it the wrong way and think I'm being difficult?"

[sub_resource type="Resource" id="Resource_aioph"]
script = ExtResource("1_p6dvn")
speaker = "Doug"
text = "I mean it's not a big deal, I guess. It sounds close enough... and I guess Duck *is* a fun-er name than plain old Doug. Should I just go by Duck from now on?"

[sub_resource type="Resource" id="Resource_hm3do"]
script = ExtResource("1_p6dvn")
speaker = "Interviewer"
text = "Duck? Is everything alright?"

[sub_resource type="Resource" id="Resource_ggn3b"]
script = ExtResource("2_p1qca")
text = "Point out that you are not a duck."
payload = "bullet_hell_1_nomask"

[sub_resource type="Resource" id="Resource_i778h"]
